{% extends 'base.html.twig' %}

{% block title %}Licencie{% endblock %}

{% block body %}
    <h1>Licencié.e</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ licencie.id }}</td>
            </tr>
            <tr>
                <th>NomLicencie</th>
                <td>{{ licencie.nomLicencie }}</td>
            </tr>
            <tr>
                <th>PrenomLicencie</th>
                <td>{{ licencie.prenomLicencie }}</td>
            </tr>
            <tr>
                <th>PhotoLicencie</th>
                <td>{{ licencie.photoLicencie }}</td>
            </tr>
            <tr>
                <th>DateNaissance</th>
                <td>{{ licencie.dateNaissance ? licencie.dateNaissance|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_licencie_index') }}">Retour à la liste</a><br>

    <a href="{{ path('app_licencie_edit', {'id': licencie.id}) }}">Modifier</a>

    {{ include('licencie/_delete_form.html.twig') }}
{% endblock %}
